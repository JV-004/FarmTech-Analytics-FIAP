# ------ ESTATÍSTICAS ------
# Simula os dados do Python
plantio <- data.frame(
cultura = c("café", "milho", "café"),
area = c(100.0, 78.5, 150.0),
insumo = c("fosfato", "herbicida", "fosfato")
)
# Mostrar dados
print("=== DADOS DE PLANTIO ===")
print(plantio)
# Cálculos
media <- mean(plantio$area)
desvio <- sd(plantio$area)
print(paste("\nMédia das áreas:", round(media, 2), "m²"))
print(paste("Desvio padrão:", round(desvio, 2), "m²"))
